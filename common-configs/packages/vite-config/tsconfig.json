{
  "include": ["src/*"],
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "dist",
    "declaration": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true
  }
}
