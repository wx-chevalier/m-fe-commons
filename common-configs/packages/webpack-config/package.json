{
  "name": "@m-fe/webpack-config",
  "version": "0.2.1",
  "description": "Shareable Webpack Configuration for all micro-frontend projects.",
  "repository": "https://github.com/wx-chevalier/m-fe-configs",
  "author": "wx-chevalier",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf example/build && cross-env NODE_ENV=production webpack --config ./example/scripts/webpack/webpack.config.prod.js",
    "build:node": "rm -rf ./example-node/build && cross-env NODE_ENV=production webpack --config ./example-node/webpack.config.js --progress",
    "build:vendor": "webpack --config ./example/scripts/webpack/webpack.config.vendor",
    "dev": "webpack serve --config ./example/scripts/webpack/webpack.config.dev.js --hot --progress",
    "dev:node": "webpack --config ./example-node/webpack.config.js --watch --progress"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@m-fe/browserslist-config": "^0.0.1",
    "@svgr/webpack": "^5.5.0",
    "autodll-webpack-plugin": "^0.4.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^5.3.2",
    "html-webpack-template": "^6.2.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "less-plugin-rewrite-import": "^0.1.1",
    "mini-css-extract-plugin": "^2.2.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "prepack-webpack-plugin": "^1.1.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "resolve-url-loader": "4.0.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.1.4",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "wasm-loader": "^1.3.0",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "webpack-theme-color-replacer": "^1.3.24",
    "workbox-webpack-plugin": "^6.2.4",
    "workerize-loader": "^1.3.0"
  },
  "devDependencies": {
    "@m-fe/babel-preset": "^0.1.0",
    "@m-fe/postcss-config": "^0.0.9",
    "@types/webpack-merge": "^4.1.5",
    "antd": "^4.16.12",
    "antd-mobile": "^2.3.4",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
